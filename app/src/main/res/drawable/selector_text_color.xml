<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">    <!--按下时的状态-->    <item android:state_pressed="true" android:color="@color/colorPrimary"/>    <!--选中时的状态-->    <item android:state_selected="true" android:color="@color/colorPrimary"/>    <!--聚焦时的状态-->    <item android:state_focused="true"  android:color="@color/colorPrimary" />    <!--无状态-->    <item android:color="@color/black" /></selector>